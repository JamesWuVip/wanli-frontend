# 生产环境配置
NODE_ENV=production

# 应用配置
VITE_APP_TITLE=万里学院前端系统
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=万里学院前端开发框架

# API配置
VITE_API_BASE_URL=https://api.wanli.ai/api
VITE_API_TIMEOUT=10000

# 开发工具配置
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_MOCK=false

# 日志配置
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOG=false

# 性能配置
VITE_ENABLE_GZIP=true
VITE_ENABLE_CDN=true