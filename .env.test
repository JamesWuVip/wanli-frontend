# 测试环境配置
NODE_ENV=test

# 应用配置
VITE_APP_TITLE=万里学院前端系统 - 测试环境
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=万里学院前端开发框架 - 测试环境

# API配置
VITE_API_BASE_URL=http://localhost:8080/api
VITE_API_TIMEOUT=15000

# 开发工具配置
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK=true

# 日志配置
VITE_LOG_LEVEL=info
VITE_ENABLE_CONSOLE_LOG=true

# 测试配置
VITE_TEST_MODE=true
VITE_ENABLE_E2E_TESTS=true
VITE_ENABLE_UNIT_TESTS=true
VITE_TEST_COVERAGE=true

# 性能配置
VITE_ENABLE_GZIP=false
VITE_ENABLE_CDN=false

# 测试数据库配置
VITE_TEST_DB_URL=http://localhost:5432/wanli_test
VITE_TEST_REDIS_URL=redis://localhost:6379/1

# 测试服务端口
VITE_TEST_PORT=3001
VITE_TEST_API_PORT=8081