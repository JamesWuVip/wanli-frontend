# 开发环境配置
NODE_ENV=development

# 应用配置
VITE_APP_TITLE=万里学院前端系统
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=万里学院前端开发框架

# API配置
VITE_API_BASE_URL=http://localhost:8080/api
